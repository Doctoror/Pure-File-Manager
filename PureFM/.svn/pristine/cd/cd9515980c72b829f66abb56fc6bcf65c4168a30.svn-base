<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ThemeLight" parent="android:Theme.Holo.Light">
    </style>
    
    <style name="NavigationText" parent="ThemeLight">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#454545</item>
    </style>
    
    <style name="browser_text_medium" parent="ThemeLight">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#454545</item>
        <item name="android:singleLine">true</item>
        <item name="android:textIsSelectable">false</item>
    </style>
    
    <style name="browser_text_small" parent="ThemeLight">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#454545</item>
        <item name="android:singleLine">true</item>
        <item name="android:textIsSelectable">false</item>
    </style>

</resources>